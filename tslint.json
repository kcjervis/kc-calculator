{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "no-bitwise": false,
    "no-shadowed-variable": false
  },
  "rules": {
    "prettier": true,
    "no-console": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "src/**/*.json"
    ]
  }
}