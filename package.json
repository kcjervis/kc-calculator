{
  "name": "kc-calculator",
  "version": "0.1.14",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "madono (https://github.com/MadonoHaru)",
  "repository": {
    "type": "git",
    "url": "https://github.com/kcjervis/kc-calculator.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "tsc -p tsconfig.build.json",
    "script": "yarn run ts-node --files scripts",
    "upload": "yarn build && yarn publish",
    "update-ships": "yarn run ts-node --files scripts",
    "db": "yarn run ts-node --files scripts/db"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "lodash-es": "^4.17.11"
  },
  "devDependencies": {
    "@types/jest": "^24.0.9",
    "@types/lodash-es": "^4.17.2",
    "@types/nedb": "^1.8.7",
    "@types/node": "^11.9.5",
    "jest": "^24.1.0",
    "nedb": "^1.8.0",
    "prettier": "^1.15.3",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.1"
  }
}
