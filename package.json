{
  "name": "kc-calculator",
  "version": "0.0.53",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "madono (https://github.com/MadonoHaru)",
  "repository": {
    "type": "git",
    "url": "https://github.com/kcjervis/kc-calculator.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "script": "yarn run ts-node --files scripts",
    "update-ships": "yarn run ts-node --files scripts",
    "upload": "yarn build && yarn publish"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@kancolle/data": "^2018.12.27",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@types/jest": "^23.3.3",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.11.4",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.2"
  }
}
